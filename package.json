{
  "name": "@mudano/ui-react",
  "version": "0.0.1",
  "description": "Mudano UI React components",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mudano/ui-react.git"
  },
  "author": "Mudano Ltd. (http://mudano.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mudano/ui-react/issues"
  },
  "homepage": "https://github.com/Mudano/ui-react#readme",
  "scripts": {
    "build": "yarn run clean-dist && NODE_ENV=production `yarn bin`/babel ./src --out-dir ./dist",
    "clean-dist": "rm -rf ./dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/test/jest/__mocks__/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "./test/jest/setupEnvironment.js"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.17",
    "@storybook/addon-info": "^3.2.17",
    "@storybook/addon-knobs": "^3.2.17",
    "@storybook/addon-links": "^3.2.17",
    "@storybook/addon-options": "^3.2.17",
    "@storybook/react": "^3.2.17",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "react": ">=0.14.0 <= 15",
    "react-dom": ">=0.14.0 <= 15",
    "react-test-renderer": "<= 15"
  },
  "peerDependencies": {
    "react": ">=0.14.0 <= 15",
    "react-dom": ">=0.14.0 <= 15"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  }
}
