{
  "name": "@mudano/ui-react",
  "version": "0.0.1",
  "description": "Mudano UI React components",
  "main": "dist/umd/ui-react.min.js",
  "jsnext:main": "dist/es/index.js",
  "module": "dist/es/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mudano/ui-react.git"
  },
  "author": "Mudano Ltd. (http://mudano.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mudano/ui-react/issues"
  },
  "homepage": "https://github.com/Mudano/ui-react#readme",
  "engines": {
    "node": ">=8.5.0"
  },
  "scripts": {
    "start": "yarn storybook:watch",
    "dev": "yarn start",
    "watch": "yarn start",
    "build": "npm-run-all -s library:clean library:build",
    "library:clean": "rimraf ./dist",
    "library:build": "npm-run-all -s lint library:build-umd library:build-es",
    "library:build-umd": "webpack",
    "library:build-es": "node es-build",
    "storybook:build": "yarn lint && build-storybook -o ./docs",
    "storybook:watch": "start-storybook -p 6006",
    "lint": "npm-run-all -s lint:scss lint:js",
    "lint:scss": "stylelint './src/**/*.scss'",
    "lint:scss-fix": "yarn lint:scss --fix",
    "lint:js": "eslint ./src --cache",
    "lint:js-fix": "yarn lint:js --fix",
    "test": "npm-run-all -s test:unit test:int",
    "test:unit": "karma start karma.config.js --autoWatch false --singleRun true",
    "test:unit-watch": "karma start karma.config.js --autoWatch true --singleRun false",
    "test:int": "codeceptjs run",
    "test:int-setup": "./test/codecept/setup",
    "selenium": "selenium-standalone"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-docgen": "^1.8.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "chai-enzyme": "^0.8.0@beta",
    "clone": "^2.1.1",
    "codeceptjs-webdriverio": "^1.1.0",
    "css-loader": "^0.28.7",
    "dirty-chai": "^2.0.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^2.0.8",
    "mocha": "^4.0.1",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "react": ">=0.14.0 <= 15",
    "react-dom": ">=0.14.0 <= 15",
    "react-svg-loader": "^2.1.0",
    "react-test-renderer": "<= 15",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "selenium-standalone": "^6.12.0",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.14.0",
    "style-loader": "^0.19.1",
    "stylelint": "^8.3.1",
    "stylelint-order": "^0.8.0",
    "stylelint-scss": "^2.2.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "uglifyjs-webpack-plugin": "^1.1.5"
  },
  "peerDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "css-loader": "^0.28.7",
    "node-sass": "^4.7.2",
    "prop-types": "15.6.0",
    "react": ">=0.14.0 <= 15",
    "react-dom": ">=0.14.0 <= 15",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "@storybook/addon-options": "^3.2.17",
    "@storybook/addons": "^3.2.17",
    "@storybook/react": "^3.2.17",
    "classnames": "^2.2.5",
    "react-showdown": "^1.6.0",
    "prop-types": "15.6.0"
  }
}
